{
    "python.analysis.extraPaths": [
        "downloads/Genesis"         
    ],
    "python.defaultInterpreterPath": "${workspaceFolder}/env_genesis/bin/python",

        "psi-header.config": {
        "forceToTop": true,
        "blankLinesAfter": 1,
        "spacesBetweenYears": false,
        "license": "Apache-2.0",
        "authorEmail": "atticlmr@gmail.com",
        "projectname": "genesislab",
        "copyrightHolder": "genesislab developer",
        "github":"https://github.com/Atticlmr/genesislab",
        "creationDateZero": "asIs",
        "hostname": "myhostname",
        "overridePathSeparator": "/"
    },
    "psi-header.changes-tracking": {
        "isActive": true,
        "modAuthor": "Modified By:",
        "modDate": "Last Modified:",
        "modDateFormat": "dd/MM/yyyy hh:nn:ss",
        "include": [],
        "includeGlob": [],
        "exclude": ["markdown", "json", "jsonc", "shellscript"],
        "excludeGlob": ["./**/*/ignoreme.*"],
        "autoHeader": "autoSave",
        "enforceHeader": true,
        "replace": [
            "Filename:",
            "Project"
        ],
        "updateLicenseVariables": false
    },
    "psi-header.variables": [
        ["manager", "Old Mother Bodkin"],
        ["projectCreationYear", "2019"],
         ["github", "https://github.com/Atticlmr/genesislab"],
         ["license", "Apache-2.0"],
    ],
    "psi-header.lang-config": [
        {
            "language": "python",
            "begin": "",
            "prefix": " # ",
            "suffix": "",
            "lineLength": 79,
            "end": "",
            "forceToTop": true,
            "blankLinesAfter": 1,
            "beforeHeader": [],
            "afterHeader": [],
            "rootDirFileName": "package.json",
            "modAuthor": "Modified By:",
            "modDate": "Last Modified:",
            "modDateFormat": "dd/MM/yyyy hh:nn:ss",
            "replace": [
                "Filename:",
                "Project"
            ],
            "ignoreLines": []
        },
    ],
    "psi-header.templates": [
        {
            "language": "python",
            "template": [
                "File: <<filename>>",
                "Project: <<projectname>>",
                "GitHub: <<github>>", 
                "Created Date: <<filecreated('dd MMM yyyy')>>",
                "Author: <<author>>",
                "-----",
                "Last Modified: <<date>>",
                "Modified By: <<author>>",
                "-----",
                "Copyright (c) <<yeartoyear(fc, now)>> <<author>>, <<company>>",
                 "License: <<license>>",
            ],
            "changeLogCaption": "HISTORY",
            "changeLogHeaderLineCount": 2,
            "changeLogEntryTemplate": [
                "",
                "<<dateformat('DD-MM-YYYY')>>\t<<initials>>\t"
            ],
            "changeLogNaturalOrder": false,
            "changeLogFooterLineCount": 0
        },
    ],

}